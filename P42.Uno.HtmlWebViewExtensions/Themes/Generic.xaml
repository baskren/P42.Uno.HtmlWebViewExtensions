            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <local:NativeWebView />
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

</ResourceDictionary>
